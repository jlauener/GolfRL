function add_e(name,x,y)
 local data=edata[name]
 local e={data=data,
									 x=x,
									 y=y,
									 hp=data.hp,
									 cnt=0,
									 frame=0,
									 acnt=0,
									 sprite=data.sprite,
									 shadow=data.shadow}
	add(elist,e)
	return e
end

